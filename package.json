{
  "name": "mockgen",
  "version": "0.0.0",
  "description": "small mock generator with semi-random value generator. easy to use",
  "main": "lib/index.js",
  "scripts": {
    "cleanup": "npm prune && npm i && npm dedupe",
    "test": "node_modules/.bin/eslint --color --debug --config ./.eslintrc.json lib/**/*.js && node_modules/.bin/jest --coverage",
    "jest": "node_modules/.bin/jest --coverage",
    "precommit": "npm test",
    "prepush": "npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aliaksandr-master/mockgen.git"
  },
  "keywords": [
    "mock",
    "stub",
    "generator",
    "gen",
    "response",
    "test",
    "data"
  ],
  "author": "alxe.master@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aliaksandr-master/mockgen/issues"
  },
  "homepage": "https://github.com/aliaksandr-master/mockgen#readme",
  "devDependencies": {
    "babel-core": "6.21.0",
    "babel-eslint": "7.1.1",
    "babel-jest": "18.0.0",
    "babel-loader": "6.2.10",
    "babel-plugin-transform-runtime": "6.15.0",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-es2016": "6.16.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-stage-2": "6.18.0",
    "babel-register": "6.18.0",
    "babel-runtime": "6.20.0",
    "chance": "1.0.10",
    "crypto-js": "3.1.9-1",
    "eslint": "4.4.1",
    "eslint-config-eslint": "4.0.0",
    "eslint-import-resolver-webpack": "0.8.3",
    "eslint-loader": "1.9.0",
    "eslint-plugin-eslint-comments": "1.0.3",
    "eslint-plugin-fp": "2.3.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsdoc": "3.1.2",
    "eslint-plugin-lodash": "2.4.4",
    "eslint-plugin-lodash-fp": "2.1.3",
    "eslint-plugin-promise": "3.5.0",
    "husky": "0.12.0",
    "jest": "18.1.0",
    "jest-cli": "18.1.0",
    "lodash": "4.17.4",
    "webpack": "1.14.0"
  },
  "peerDependencies": {
    "chance": "1.0.10",
    "crypto-js": "3.1.9-1",
    "lodash": "4.17.4"
  },
  "dependencies": {
    "localforage": "1.4.3",
    "normalizr": "3.1.0",
    "urijs": "1.18.4"
  },
  "jest": {
    "automock": false,
    "notify": true,
    "testEnvironment": "jsdom",
    "cacheDirectory": ".tmp/jest-cache",
    "coverageDirectory": ".tmp/tests-coverage-report",
    "collectCoverageFrom": [
      "**/*.{js,jsx}"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/__tests__/"
    ],
    "moduleFileExtensions": [
      "",
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "verbose": false
  }
}
